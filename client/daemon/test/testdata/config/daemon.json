{
  "alive_time": "0s",
  "gc_interval": "1m0s",
  "pid_file": "/tmp/dfdaemon.pid",
  "lock_file": "/tmp/dfdaemon.lock",
  "data_dir": "/tmp/dragonfly/dfdaemon/",
  "work_home": "/tmp/dragonfly/dfdaemon/",
  "keep_storage": false,
  "schedulers": [
    {
      "type": "tcp",
      "addr": "127.0.0.1:8002"
    }
  ],
  "verbose": true,
  "host": {
    "listen_ip": "0.0.0.0",
    "advertise_ip": "0.0.0.0",
    "location": "0.0.0.0",
    "idc": "d7y",
    "security_domain": "d7y.io",
    "net_topology": "d7y"
  },
  "download": {
    "rate_limit": "200Mi",
    "download_grpc": {
      "security": {
        "insecure": true,
        "ca_cert": "",
        "cert": "",
        "key": "",
        "tls_config": null
      },
      "unix_listen": {
        "socket": "/tmp/dfdamon.sock"
      }
    },
    "peer_grpc": {
      "security": {
        "insecure": true,
        "cacert": "",
        "cert": "",
        "key": "",
        "tls_config": null
      },
      "tcp_listen": {
        "listen": "0.0.0.0",
        "port": 65000
      }
    }
  },
  "upload": {
    "rate_limit": "100Mi",
    "security": {
      "insecure": true,
      "cacert": "",
      "cert": "",
      "key": ""
    },
    "tcp_listen": {
      "listen": "0.0.0.0",
      "port": 65002
    }
  },
  "storage": {
    "task_expire_time": "3m0s",
    "strategy": "io.d7y.storage.v2.simple"
  },
  "proxy": {
    "registry_mirror": {
      "url": "https://index.docker.io",
      "insecure": false,
      "certs": [],
      "direct": false
    },
    "proxies": [
      {
        "regx": "blobs/sha256.*"
      },
      {
        "regx": "some-registry/",
        "use_https": true
      },
      {
        "regx": "no-proxy-reg",
        "direct": true
      },
      {
        "regx": "some-registry",
        "redirect": "another-registry"
      }
    ],
    "hijack_https": {
      "cert": "",
      "key": "",
      "hosts": [
        {
          "regx": "mirror.aliyuncs.com:443",
          "insecure": false,
          "certs": []
        }
      ]
    }
  }
}
