{
  "registry_mirror": {
    "remote": "https://index.docker.io",
    "insecure": false,
    "certs": [],
    "direct": false
  },
  "proxies": [
    {
      "regx": "blobs/sha256.*"
    },
    {
      "regx": "some-registry/",
      "use_https": true
    },
    {
      "regx": "no-proxy-reg",
      "direct": true
    },
    {
      "regx": "some-registry",
      "redirect": "another-registry"
    }
  ],
  "hijack_https": {
    "cert": "../certs/sca.crt",
    "key": "../certs/sca.key",
    "hosts": [
      {
        "regx": "mirror.aliyuncs.com:443",
        "insecure": false,
        "certs": []
      }
    ]
  }
}
